
@model VPSA.Models.UserLoginModel
@{
    Layout = null;
}

<title>VPSA</title>


<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Bootstrap, from Twitter</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css">
    <style>
        .img-logo {
            max-height: 60px;
            margin-right: 15px
        }

        h1 {
            color: #2990CA;
        }

        .btn-outline-custom {
            color: white;
            background-color: #2990CA;
        }

            .btn-outline-custom:hover {
                color: white;
                background-color: #2990CA;
            }
    </style>
</head>
<body style="background-color: rgba(0,0,0,0.2);">
    <main role="main" class="container" style="margin-top: 150px;">
        <div class="row">
            <div class="col-lg-8 offset-lg-4 col-md-8 offset-md-3 col-12">
                <div class="form-inline">
                    <img class="img-logo" src="data:image/svg+xml;base64,
      " />
                    <h1 class="my-0 font-weight-normal">Municipio</h1>
                    <h1 class="my-0 mr-md-auto font-weight-bold">VPSA</h1>
                </div>
            </div>
        </div>
        <br />
        <div class="row">
            <div class="card col-lg-4 offset-lg-4 col-md-6 offset-md-3 col-12">
                <div class="card-body">
                    @* <div id="login" class="col-lg-4 offset-lg-4 col-md-6 offset-md-3 col-12">*@
                    <h2 class="text-center">Iniciar Sesión</h2>
                    <br />
                    <form asp-action="Login" asp-route-returnUrl="@ViewData["ReturnUrl"]">
                        <div class="form-group">
                            <label for="Legajo">Legajo</label>
                            <input asp-for="Legajo" class="form-control" />
                            <span asp-validation-for="Legajo" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label for="palabraSecreta">Contraseña</label>
                            <input asp-for="Password" class="form-control" />
                            <span asp-validation-for="Password" class="text-danger"></span>
                        </div>
                        @*<div class="form-group form-check">
                                <label class="form-check-label">
                                    <input class="form-check-input" asp-for="RememberMe" /> @Html.DisplayNameFor(model => model.RememberMe)
                                </label>
                            </div>*@
                        <button type="submit" class="btn btn-outline-custom mb-2 float-right">
                            Entrar
                        </button>
                        <br>
                        @*<a href="#">Contraseña olvidada</a>*@
                    </form>
                    @*</div>*@
                </div>
            </div>

        </div>
        <div class="row">
            <div class="col-lg-4 offset-lg-4 col-md-6 offset-md-3 col-12">
                <a href="/Home" class="btn-link">Ir a página Principal</a>
            </div>
        </div>
    </main>
</body>

</html>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

